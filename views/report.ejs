            var opt = {
                margin: 0.6,
                filename: 'alubee_hourly_report.pdf',
                html2canvas: { scale: 6 },
                jsPDF: { unit: 'in', format: [12, 15], orientation: 'portrait' }
            };

            // New Promise-based usage:
